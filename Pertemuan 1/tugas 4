angka = int(input("Masukkan angka: "))

if angka < 2:
    print("Bukan angka prima")
else:
    prima = True
    for i in range(2, int(angka ** 0.5) + 1):
        if angka % i == 0:
            prima = False
            break
    if prima:
        print("Angka prima")
    else:
        print("Bukan angka prima")
